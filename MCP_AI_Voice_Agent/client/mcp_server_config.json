{
  "mcpServers": {
    "time": {
      "command": "python",
      "args": [
        "-m",
        "mcp_server_time",
        "--local-timezone=Asia/Shanghai"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server"
      ]
    },
    "music_player": {
      "command": "python",
      "args": [
        "/<Path>/MCP/server/mplayer/main.py"
      ]
    }
  }
}
